{
  // Default settings
  // Ref: https://code.visualstudio.com/docs/getstarted/settings

  // Core
  // Ref: https://code.visualstudio.com/docs/supporting/faq#_how-to-disable-telemetry-reporting
  "telemetry.telemetryLevel": "off",
  "editor.accessibilitySupport": "off",
  "security.workspace.trust.untrustedFiles": "open",

  // Settings as JSON
  "workbench.settings.useSplitJSON": true,
  "workbench.settings.editor": "json",

  // Enable/disable default JSON formatter
  "json.format.enable": true,

  // At Startup
  "workbench.startupEditor": "none",

  // Do not show the notifications for extension recommendations
  "extensions.closeExtensionDetailsOnViewChange": true,
  "extensions.ignoreRecommendations": true,

  // UI
  "breadcrumbs.enabled": true,
  "editor.tabSize": 2,
  "window.title": "${activeEditorShort}${separator}${rootName}",
  "window.titleBarStyle": "native",
  "window.nativeTabs": true,
  "window.zoomLevel": -0.499,
  "editor.insertSpaces": true,
  "editor.scrollBeyondLastLine": false,
  "editor.padding.bottom": 644,
  "editor.renderFinalNewline": "on",
  "editor.renderControlCharacters": true,
  "editor.guides.indentation": true,
  "editor.renderLineHighlight": "all",
  "workbench.editor.tabCloseButton": "left",
  "workbench.editor.showIcons": true,
  "workbench.editor.enablePreview": false,
  "problems.decorations.enabled": false,
  "editor.bracketPairColorization.enabled": false,
  "workbench.activityBar.visible": false,
  "editor.linkedEditing": true,
  "tabnine.experimentalAutoImports": true,

  // Indentation File Tree Structure
  "workbench.tree.indent": 17,

  // Suggestion delay so you can still have tips but they don't appear
  // unless you hover over the element for some time
  "editor.quickSuggestionsDelay": 1000,

  // Controls whether the minimap is shown
  "editor.minimap.showSlider": "always",
  "editor.minimap.enabled": true,
  "editor.minimap.renderCharacters": false,
  "editor.minimap.scale": 2,
  "editor.minimap.maxColumn": 140,

  // Controls the cursor style
  "editor.cursorStyle": "line-thin",

  // Links
  "editor.links": false,
  "editor.snippetSuggestions": "top",
  "editor.tabCompletion": "onlySnippets",

  // Controls how the editor should render whitespace
  "files.eol": "\n",
  "editor.renderWhitespace": "all",
  "code-eol.newlineCharacter": "¬",
  "code-eol.returnCharacter": "¤",
  "code-eol.crlfCharacter": "¤¬",
  "files.trimTrailingWhitespace": true,
  "editor.rulers": [80, 120],

  // Controls how lines should wrap.
  // - off: Lines will never wrap.
  // - on: Lines will wrap at the viewport width.
  // - wordWrapColumn: Lines will wrap at `editor.wordWrapColumn`.
  // - bounded: Lines will wrap at the minimum of viewport and `editor.wordWrapColumn`.
  "editor.wordWrap": "bounded",

  // Controls the wrapping column of the editor when `editor.wordWrap` is `wordWrapColumn` or `bounded`.
  "editor.wordWrapColumn": 120,

  // Controls the sizing of editor tabs. This value is ignored when `workbench.editor.showTabs` is disabled.
  // - fit: Always keep tabs large enough to show the full editor label.
  // - shrink: Allow tabs to get smaller when the available space is not enough to show all tabs at once.
  "workbench.editor.tabSizing": "shrink",

  // Gutter
  "scm.diffDecorationsGutterWidth": 5,
  "scm.diffDecorations": "gutter",
  "merge-conflict.diffViewPosition": "Below",
  "editor.showFoldingControls": "mouseover",

  // Scrollbar
  "editor.scrollbar.horizontal": "hidden",
  "editor.scrollbar.vertical": "hidden",
  "editor.scrollbar.horizontalScrollbarSize": 0,
  "editor.scrollbar.verticalScrollbarSize": 0,
  "editor.overviewRulerBorder": true,

  // Typography
  "editor.fontFamily": "'SF Mono', Menlo, monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 13,
  "editor.lineHeight": 19,
  "editor.fontWeight": "500",
  "workbench.fontAliasing": "antialiased",

  // Markdown preview
  "markdown-preview-enhanced.enableExtendedTableSyntax": true,
  "markdown-preview-enhanced.scrollSync": true,
  //"markdown.preview.breaks": true,
  "markdown.preview.doubleClickToSwitchToEditor": false,
  "markdown.preview.fontFamily": "'Helvetica', -apple-system, BlinkMacSystemFont, sans-serif",
  "markdown.preview.fontSize": 16,
  "markdown.preview.lineHeight": 1.6,

  // Highlighting
  "editor.occurrencesHighlight": false, // Syntax highlighting (place cursor inside variable)
  "editor.selectionHighlight": true, // Selection highlighting (similar chunks in document)

  // Terminal
  "terminal.integrated.tabs.title": "${process}",
  "terminal.integrated.tabs.description": "${task}${separator}${local}${separator}${cwdFolder}",
  "terminal.explorerKind": "integrated",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorWidth": 1,
  "terminal.integrated.cursorStyle": "underline",
  "terminal.external.osxExec": "Terminal.app",
  "terminal.integrated.fontSize": 13,
  "terminal.integrated.lineHeight": 1.2,

  // Prettier
  "prettier.configPath": "~/Library/Application Support/Code/User/configExtensions/prettierrc.json",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.formatOnPaste": false,
  "prettier.printWidth": 120,

  // Language Scopes
  "[css]": {
    "editor.formatOnSave": true,
    "editor.renderWhitespace": "all",
    "editor.defaultFormatter": "mrmlnc.vscode-csscomb"
  },
  "[less]": {
    "editor.formatOnSave": true,
    "editor.renderWhitespace": "all",
    "editor.defaultFormatter": "mrmlnc.vscode-csscomb"
  },
  "[sass]": {
    "editor.formatOnSave": true,
    "editor.renderWhitespace": "all",
    "editor.defaultFormatter": "mrmlnc.vscode-csscomb"
  },
  "[scss]": {
    "editor.formatOnSave": true,
    "editor.renderWhitespace": "all",
    "editor.defaultFormatter": "mrmlnc.vscode-csscomb"
  },
  "[markdown]": {
    "editor.quickSuggestions": {
      "comments": "off",
      "strings": "off",
      "other": "off"
    },
    "editor.formatOnSave": true,
    "editor.wordWrap": "on",
    "editor.acceptSuggestionOnEnter": "off",
    "editor.defaultFormatter": "vscode.markdown-language-features"
  },
  "[mdx]": {
    "editor.quickSuggestions": {
      "comments": "off",
      "strings": "off",
      "other": "off"
    },
    "editor.wordWrap": "on"
  },
  "[typescript]": {
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true
  },

  // GitLens
  //"gitlens.mode.active": "zen",
  "editor.codeLens": false,
  "gitlens.codeLens.enabled": false,
  "gitlens.codeLens.recentChange.enabled": false,
  "gitlens.codeLens.authors.enabled": false,
  "gitlens.currentLine.enabled": false,

